    "window.dialogStyle": "custom",
    "search.followSymlinks": false,
    "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
    "files.associations": {
        "*.txt": "python"
    },
    "vim.easymotion": true,
    "vim.incsearch": true,
    "vim.useSystemClipboard": true,
    "vim.useCtrlKeys": true,
    "vim.neovimUseConfigFile": true,
    "vim.searchHighlightColor": "#FFD242",
    "vim.searchHighlightTextColor": "#23230F",
    "vim.cursorStylePerMode.replace": "block-outline",
    "vim.cursorStylePerMode.visualblock": "underline",
    "vim.enableNeovim": true,
    "vim.hlsearch": true,
    "vim.neovimConfigPath": "~/.config/nvim/init.lua",
    "vim.neovimPath": "/opt/homebrew/bin/nvim",
    "vim.normalModeKeyBindingsNonRecursive": [
        {
            "before": [
                "<leader>",
                "d"
            ],
            "after": [
                "d",
                "d"
            ]
        },
        {
            "before": [
                "<leader>",
                "n"
            ],
            "commands": [
                ":nohl"
            ]
        },
        {
            "before": [
                "K"
            ],
            "commands": [
                "lineBreakInsert"
            ],
            "silent": true
        }, {
            "before": [
                "leader",
                "h"
            ],
            "commands": [
                "workbench.action.toggleSidebarVisibility"
            ]
        }, {
            "before": [
                "leader",
                "e"
            ],
            "commands": [
                "workbench.view.explorer"
            ],
            "silent": true
        } 
    ],
    "vim.leader": "<space>",
    "vim.handleKeys": {
        "<C-v>": true,
        "<C-a>": false,
        "<C-s>": false,
        "<C-z>": false,
        "<C-f>": false,
        "<C-y>": false,
    },
    "security.workspace.trust.untrustedFiles": "open",
    "git.enableSmartCommit": true,
    "gitlens.codeLens.authors.enabled": false,
    "gitlens.codeLens.enabled": false,
    "gitlens.codeLens.recentChange.enabled": false,
    "i18n-ally.autoDetection": false,
    "i18n-ally.displayLanguage": "en",
    "i18n-ally.ignoredLocales": [],
    // leetcode
    "leetcode.endpoint": "leetcode-cn",
    "leetcode.workspaceFolder": "C:\\Users\\sxh\\.leetcode",
    "leetcode.defaultLanguage": "javascript",
    "leetcode.hint.commandShortcut": false,
    "leetcode.hint.configWebviewMarkdown": false,
    "files.eol": "\n",
    "explorer.confirmDragAndDrop": false,
    "explorer.copyRelativePathSeparator": "/",
    "markdown.preview.fontSize": 13,
    "window.commandCenter": true,
    "eslint.codeAction.showDocumentation": {
        "enable": true
    },
    "eslint.quiet": true,
    "eslint.format.enable": true,
    "eslint.run": "onSave",
    "eslint.validate": [
        "javascript",
        "javascriptreact",
        "vue",
        "html",
        "markdown",
        "jsonc",
        "json5"
    ],
    "prettier.enable": false,
